{
  "name": "qnet-wallet",
  "version": "1.0.0",
  "description": "QNet Wallet Chrome Extension with post-quantum security",
  "main": "popup.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "clean": "rimraf dist node_modules/**/*test* node_modules/**/*.pem",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "package": "npm run clean && npm run build && cd dist && powershell Compress-Archive -Path * -DestinationPath ../qnet-wallet-production-clean.zip -Force && cd .. && echo 'Clean package created: qnet-wallet-production-clean.zip'"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.95.7",
    "bip39": "^3.1.0",
    "bs58": "^6.0.0",
    "ed25519-hd-key": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-private-methods": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "babel-loader": "^9.2.1",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^12.0.2",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.44.0",
    "jest": "^29.6.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "webpack": "^5.99.9",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "blockchain",
    "wallet",
    "chrome-extension",
    "solana",
    "qnet"
  ],
  "author": "QNet Development Team",
  "license": "MIT"
}
