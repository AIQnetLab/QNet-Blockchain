{
  "name": "qnet-mobile",
  "version": "2.0.0",
  "description": "QNet Mobile Wallet - Dual Network (Solana/QNet) Production Application",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace QNetMobile.xcworkspace -scheme QNetMobile -configuration Release -archivePath QNetMobile.xcarchive archive",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-native": "^0.72.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-screens": "^3.27.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-gesture-handler": "^2.13.4",
    "@react-native-async-storage/async-storage": "^1.19.5",
    "@solana/web3.js": "^1.87.6",
    "crypto-js": "^4.2.0",
    "buffer": "^6.0.3",
    "react-native-crypto": "^2.2.0",
    "react-native-randombytes": "^3.6.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-svg": "^13.14.0",
    "react-native-qrcode-generator": "^1.4.0",
    "react-native-camera": "^4.2.1",
    "react-native-permissions": "^3.10.1",
    "react-native-biometrics": "^3.0.1",
    "react-native-keychain": "^8.1.3",
    "react-native-device-info": "^10.11.0",
    "react-native-share": "^9.4.1",
    "react-native-clipboard": "^1.5.1",
    "react-native-haptic-feedback": "^2.2.0",
    "@react-native-community/netinfo": "^9.4.1",
    "react-native-reanimated": "^3.5.4",
    "react-native-linear-gradient": "^2.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/runtime": "^7.23.2",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QNet-Project/QNet-Mobile.git"
  },
  "keywords": [
    "qnet",
    "solana",
    "blockchain",
    "mobile",
    "wallet",
    "react-native",
    "cryptocurrency",
    "dual-network",
    "phase2",
    "activation"
  ],
  "author": "QNet Development Team",
  "license": "MIT",
  "private": false,
  "react-native": {
    "crypto": "react-native-crypto",
    "stream": "stream-browserify",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough"
  },
  "browser": {
    "crypto": "crypto-browserify",
    "stream": "stream-browserify"
  }
} 